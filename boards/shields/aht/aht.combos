/ {
	combos {
		compatible = "zmk,combos";

		combo_to_base {
			timeout-ms = <100>;
			key-positions = <0 1 2>;
			bindings = <&to 0>;
		};

		combo_to_studio {
			timeout-ms = <100>;
			key-positions = <3 4 5>;
			bindings = <&to 1>;
		};

		combo_to_media {
			timeout-ms = <100>;
			key-positions = <6 7 8>;
			bindings = <&to 2>;
		};

		combo_studio_unlock {
			timeout-ms = <150>;
			key-positions = <0 1>;
			bindings = <&studio_unlock>;
		};
	};
};
